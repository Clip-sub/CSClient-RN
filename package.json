{
  "name": "csclient",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "node_modules/.bin/flow",
    "checkdeps": "ncu --packageFile package.json",
    "i5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "i6": "react-native run-ios --simulator=\"iPhone 6\"",
    "a": "react-native run-android",
    "ar": "react-native run-android --variant=\"release\"",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    //"precommit": "lint-staged && npm run git-hook",
    //"prepush": "npm run git-hook",
    "git-hook": "npm test -s",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "eslint-fix": "eslint --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apisauce": "^0.14.1",
    "base-64": "^0.1.0",
    "bugsnag-react-native": "^2.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "native-base": "^2.3.3",
    "querystringify": "^1.0.0",
    "react": "16.1.1",
    "react-native": "0.50.3",
    "react-native-animatable": "^1.2.4",
    "react-native-bottomsheet": "^1.7.7",
    "react-native-code-push": "^5.1.3-beta",
    "react-native-config": "^0.9.1",
    "react-native-device-info": "0.12.1",
    "react-native-global-props": "^1.1.3",
    "react-native-i18n": "1.0.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-onesignal": "^3.0.7",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-vector-icons": "^4.4.2",
    "react-navigation": "1.0.0-beta.19",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-persist": "^5.3.5",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.2",
    "util": "^0.10.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "babel-eslint": "^8.0.2",
    "babel-jest": "21.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^9.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-jest": "^21.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-syntax": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "eslint-plugin-sort-class-members": "^1.2.2",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "lint-staged": "^5.0.0",
    "mockery": "^2.1.0",
    "prettier": "^1.8.2",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-test-renderer": "^16.1.1"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|bugsnag-react-native|static-container|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}